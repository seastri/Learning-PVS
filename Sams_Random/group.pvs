group: THEORY
  BEGIN 
    G: TYPE+
    e: G
    i: [G -> G]
    *: [G,G -> G]
    x,y,z : VAR G
    associative : AXIOM
        (x * y) * z = x * (y * z)
    id_left : AXIOM
        i(x) * x = e
    inverse_associative : THEOREM
        i(x) * (x*y) = y
  END group
